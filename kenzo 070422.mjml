            <mj-image width="100px" src="https://sastdassetspimweuprd.blob.core.windows.net/assets-pim-prd/E-Ticket/LOGO-TAG-COULEUR-NO-FRAME-RVB.png?si=KZ-OCTIPAS-PIM-PRD&sv=2019-02-02&sr=c&sig=onuMsL%2BVoory0J5pmOpx37DQxUflUWZqxOdqeNe%2BjKA%3D"></mj-image>
            <mj-raw>{% endif %}</mj-raw>
            <mj-navbar>
              <mj-navbar-link href="https://www.kenzo.com/eu/fr/recherche-magasin" font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">{{ language['cta_1'] }}</mj-navbar-link>
              <mj-navbar-link href="https://www.kenzo.com/eu/fr/hommes" font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">{{ language['cta_2'] }}</mj-navbar-link>
              <mj-navbar-link href="https://www.kenzo.com/eu/fr/femmes" font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">{{ language['cta_3'] }}</mj-navbar-link>
              <mj-navbar-link href="https://www.kenzo.com/eu/fr/accessoires-1" font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">{{ language['cta_4'] }}</mj-navbar-link>
            </mj-navbar>
            <mj-divider></mj-divider>
    
            <mj-image src="https://sastdassetspimweuprd.blob.core.windows.net/assets-pim-prd/E-Ticket/TICKET_CAISSE_DIGITAL_D4.jpg?si=KZ-OCTIPAS-PIM-PRD&sv=2019-02-02&sr=c&sig=onuMsL%2BVoory0J5pmOpx37DQxUflUWZqxOdqeNe%2BjKA%3D"></mj-image>
    
            <mj-divider></mj-divider>
    
            <mj-text font-size="18px" align="center" font-family="Gitan Latn, futura-pt, Times New Roman">{{ language['Thankyou_1'] }}
              <br /><br />{{ language['Thankyou_2'] }}<br />{{ language['Thankyou_3'] }}
            </mj-text>
            <mj-divider></mj-divider>
            <mj-text font-size="12px" align="left" font-family="Gitan Latn, futura-pt, Times New Roman" line-height="1.5">{{ language['date_of_purchase'] }} {{receipt.get_local_time().strftime('%d/%m/%Y')}}<br />{{ language['transaction_number'] }} {{ data['receipt']['transaction_number']['text'] }}<br />{{ data['store']['name'] }}</mj-text>
            <mj-table font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">
              <tr>
                <th style="text-align: left; vertical-align: top; width: 20%;"></th>
                <th style="text-align: left; vertical-align: top; width: 55%;">{{ language['description'] }}</th>
                <th style="text-align: center; vertical-align: top; width: 10%;">{{ language['quantity'] }}</th>
                <th style="text-align: right; vertical-align: top; width: 15%;">{{ language['price'] }}</th>
              </tr>
              <mj-raw>{% for sale_item in receipt['sale_item'] %}</mj-raw>
              <tr style="border-top: 1px solid #000000">
                <td style="text-align: left; vertical-align: top; width: 20%;">{{ sale_item['item_alternate_id'][:12] }}</td>
                <td style="text-align: left; vertical-align: top; width: 55%;">{{ sale_item['text'] }}</td>
                <td style="text-align: center; vertical-align: top; width: 10%;">{{ sale_item['quantity']['value'] }}</td>
                <td style="text-align: right; vertical-align: top; width: 15%;">{{ receipt.format_currency(sale_item['regular_price']['value']) }}</td>
              </tr>
    
              <mj-raw>{% if sale_item['discount'] %}</mj-raw>
              <mj-raw>{% for discount in sale_item['discount'] %}</mj-raw>
              <tr>
    
                <td colspan="3" style="text-align: right; vertical-align: top;">
                  {{ discount['text'] }}&nbsp; &nbsp; {{ discount['reduction_percent']['value']|int }}%
                </td>
                <td style="text-align: right; vertical-align: top; width: 15%;">{{ format_currency(discount['amount']['value']) }}</td>
              </tr>
              <mj-raw>{% endfor %}</mj-raw>
              <tr>
                <td colspan="3" style="text-align: right; vertical-align: top;">
                  {{ language['net_amount'] }}
                </td>
                <td style="text-align: right; vertical-align: top; width: 15%;">{{ receipt.format_currency(sale_item['total']['value']) }}</td>
    
              </tr>
              <mj-raw>{% endif %}</mj-raw>
              <mj-raw>{% endfor %}</mj-raw>
            </mj-table>
          </mj-column>
        </mj-section>
        <!-- END HEADER/NAV -->
        <!-- DISCOUNTS -->
        <mj-raw>{% if receipt['discount'] %}</mj-raw>
        <mj-section>
          <mj-column>
            <mj-table font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">
              <tr>
                <th style="text-align: left; vertical-align: top;">{{ language['discount'] }}</th>
                <th style="text-align: right; vertical-align: top;">{{ language['discount_amount'] }}</th>
              </tr>
              <mj-raw>{% for discount in receipt['discount'] %}</mj-raw>
              <tr style="border-top: 1px solid #000000">
                <td style="text-align: left; vertical-align: top;">{{ discount['text'] }}</td>
                <td style="text-align: right; vertical-align: top;">{{ receipt.format_currency(discount['amount']['value']) }}</td>
              </tr>
              <mj-raw>{% endfor %}</mj-raw>
            </mj-table>
          </mj-column>
        </mj-section>
        <mj-raw>{% endif %}</mj-raw>
        <!-- END DISCOUNTS -->
    
    
        <!-- TOTAL -->
        <mj-raw>{% if receipt['receipt']['total']['amount']['value'] %}</mj-raw>
        <mj-section>
          <mj-column>
            <mj-divider border-width="1px"></mj-divider>
            <mj-table font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">
              <tr>
                <td align="left"><b>{{ language['total'] }}</b></td>
                <td align="right">{{ receipt.format_currency(data['receipt']['total']['amount']['value']) }}</td>
              </tr>
            </mj-table>
            <mj-divider border-width="1px"></mj-divider>
            <mj-raw>{% endif %}</mj-raw>
            <mj-raw>{% if receipt['tax'] %}</mj-raw>
            <mj-table font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">
              <tr>
                <th style="text-align: left; vertical-align: top; width: 25%;"></th>
                <th style="text-align: center; vertical-align: top; width: 25%;">{{ language['tax_rate'] }}</th>
                <th style="text-align: center; vertical-align: top; width: 25%;">{{ language['taxable_amount'] }}</th>
                <th style="text-align: right; vertical-align: top; width: 25%;">{{ language['tax_paid'] }}</th>
              </tr>
              <mj-raw>{% for tax in receipt['tax'] %}</mj-raw>
              <tr style="border-top: 1px solid #000000">
                <td style="text-align: left; vertical-align: top; width: 25%;"></td>
                <td style="text-align: center; vertical-align: top; width: 25%;">{{ tax['rate']['value']|int }}%</td>
                <td style="text-align: center; vertical-align: top; width: 25%;">{{ receipt.format_currency(data['receipt']['subtotal']['amount']['value']) }}</td>
                <td style="text-align: right; vertical-align: top; width: 25%;">{{ receipt.format_currency(tax['amount']['value']) }}</td>
              </tr>
              <mj-raw>{% endfor %}</mj-raw>
              <mj-raw>{% endif %}</mj-raw>
            </mj-table>
            <mj-raw>{% if receipt['tender'] %}</mj-raw>
            <mj-table font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">
              <tr>
                <th style="text-align: left; vertical-align: top;">{{ language['tender_type'] }}</th>
                <th style="text-align: right; vertical-align: top;">{{ language['tender_amount'] }}</th>
              </tr>
              <mj-raw>{% for tender in receipt['tender'] %}</mj-raw>
              <tr style="border-top: 1px solid #000000">
                <td style="text-align: left; vertical-align: top;">{{ tender['text'] }}</td>
                <td style="text-align: right; vertical-align: top;">{{ receipt.format_currency(tender['amount']['value']) }}</td>
              </tr>
              <mj-raw>{% endfor %}</mj-raw>
            </mj-table>
          </mj-column>
        </mj-section>
        <mj-raw>{% endif %}</mj-raw>
        <!-- END TOTAL -->
    
        <!-- TAXES -->
    
        <!-- END TAXES -->
    
        <!-- END TENDER -->
    
        <!-- SECOND RECEIPT TEXT
        <mj-section>
          <mj-column>
            <mj-text align="center">{{nl2br(receipt.second_receipt_text)}}</mj-text>
          </mj-column>
        </mj-section>
       END SECOND RECEIPT TEXT -->
    
        <!-- BARCODE -->
        <mj-section>
          <mj-column>
            <mj-image width="300px" src="{{ barcode_url }}" />
          </mj-column>
        </mj-section>
        <!-- END BARCODE -->
    
        <!-- CTA1 -->
        <mj-section>
          <mj-column>
            <mj-raw>{% if receipt['store']['external_id'] in ['565756', '563947', '566510', '516195', '557445', '565696', '565819'] %}</mj-raw>
            <mj-text font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">{{ language['tc_title'] }}<br /><br />{{ language['oc_1'] }}<br /><br />{{ language['oc_2'] }}<br /><br />{{ language['oc_3'] }}<br /><br />{{ language['oc_4'] }}<br /><br />{{ language['oc_5'] }}</mj-text>
            <mj-raw>{% else %}</mj-raw>
            <mj-text font-family="Gitan Latn, futura-pt, Times New Roman" font-size="12px">{{ language['tc_title'] }}<br /><br />{{ language['tc_1'] }}<br /><br />{{ language['tc_2'] }}<br /><br />{{ language['tc_3'] }}<br /><br />{{ language['tc_4'] }}<br /><br />{{ language['tc_5'] }}</mj-text>
            <mj-raw>{% endif %}</mj-raw>
          </mj-column>
        </mj-section>
        <!-- END CTA1 -->
    
    
    
        <!-- SOCIAL MEDIA -->
        <mj-section>
          <mj-column>
            <mj-social font-size="15px" icon-size="30px" mode="horizontal">
              <mj-social-element src="https://sastdassetspimweuprd.blob.core.windows.net/assets-pim-prd/E-Ticket/logo-instagram-noir.png?si=KZ-OCTIPAS-PIM-PRD&sv=2019-02-02&sr=c&sig=onuMsL%2BVoory0J5pmOpx37DQxUflUWZqxOdqeNe%2BjKA%3D" href="https://www.instagram.com/kenzo/"></mj-social-element>
            </mj-social>
          </mj-column>
        </mj-section>
        <!-- END SOCIAL MEDIA -->
      </mj-body>
